<main>
  <section class="all-users">
    <div class="add_post">
      <h1>All Users</h1>
      <button class="add_post_button">
        <%= link_to "Add post", new_post_path %>
      </button>
    </div>
    <% @users.each do |user| %>
      <%= render "shared_view/user", user: user %>
      <br>
    <% end %>
  </section>
</main>
